{
  "dashboard": {
    "id": null,
    "title": "Gogidix AI Services - Comprehensive Intelligence Dashboard",
    "tags": ["gogidix", "ai", "intelligence", "48-services"],
    "timezone": "browser",
    "panels": [
      {
        "title": "AI Services Overview - 48+ Services Status",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(up{job=\"gogidix-ai-services\"})",
            "legendFormat": "Active AI Services"
          },
          {
            "expr": "sum(rate(ai_requests_total{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "AI Requests/sec"
          },
          {
            "expr": "sum(rate(ai_predictions_total{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "AI Predictions/sec"
          },
          {
            "expr": "avg(ai_model_accuracy{job=\"gogidix-ai-services\"}) * 100",
            "legendFormat": "Avg Model Accuracy %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "showLabels": true,
          "showValues": true,
          "colorMode": "value"
        }
      },
      {
        "title": "Category 1: Property Management Automation - Performance",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total{category=\"property-management\",service=~\"property-description|photo-enhancement|virtual-staging|virtual-tour|seo-optimization|social-media|automated-valuation|market-analysis|investment-analysis\"}[5m])) by (service)",
            "legendFormat": "{{service}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per Second"
          }
        ]
      },
      {
        "title": "Category 1: Property Management - Accuracy & Success Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "avg(ai_model_accuracy{category=\"property-management\",service=~\"property-description|photo-enhancement|virtual-staging|virtual-tour|seo-optimization|social-media|automated-valuation|market-analysis|investment-analysis\"}) by (service) * 100",
            "legendFormat": "{{service}} Accuracy %"
          },
          {
            "expr": "sum(rate(ai_requests_successful{category=\"property-management\",service=~\"property-description|photo-enhancement|virtual-staging|virtual-tour|seo-optimization|social-media|automated-valuation|market-analysis|investment-analysis\"}[5m])) by (service) / sum(rate(ai_requests_total{category=\"property-management\",service=~\"property-description|photo-enhancement|virtual-staging|virtual-tour|seo-optimization|social-media|automated-valuation|market-analysis|investment-analysis\"}[5m])) by (service) * 100",
            "legendFormat": "{{service}} Success Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage (%)",
            "max": 100,
            "min": 0
          }
        ]
      },
      {
        "title": "Category 2: Smart Search & Recommendations - Performance",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total{category=\"smart-search\",service=~\"property-matching|natural-language|recommendation-engine|budget-optimization|amenity-search\"}[5m])) by (service)",
            "legendFormat": "{{service}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per Second"
          }
        ]
      },
      {
        "title": "Category 3: Lead Management & Sales - Conversion Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(ai_lead_conversions_total{category=\"lead-management\",service=~\"lead-scoring|lead-qualification|followup-automation|chatbot-assistant|appointment-scheduling|document-generation\"}[5m])) by (service)",
            "legendFormat": "{{service}} Conversions/min"
          },
          {
            "expr": "avg(ai_lead_score{category=\"lead-management\"}) by (service)",
            "legendFormat": "{{service}} Avg Lead Score"
          }
        ],
        "yAxes": [
          {
            "label": "Conversions per Minute"
          }
        ]
      },
      {
        "title": "Category 4: Market Intelligence - Insights Generated",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 8,
          "h": 6
        },
        "targets": [
          {
            "expr": "sum(rate(ai_insights_generated_total{category=\"market-intelligence\",service=~\"price-trend-analysis|supply-demand-analytics|competitor-intelligence|economic-impact|development-hotspots\"}[5m]))",
            "legendFormat": "Insights/min"
          },
          {
            "expr": "sum(increase(ai_market_predictions_total{service=~\"price-trend-analysis\"}[1h]))",
            "legendFormat": "Price Predictions/hr"
          }
        ]
      },
      {
        "title": "Category 5: Property Development Automation",
        "type": "table",
        "gridPos": {
          "x": 8,
          "y": 24,
          "w": 8,
          "h": 6
        },
        "targets": [
          {
            "expr": "up{category=\"property-development\"}",
            "legendFormat": "{{instance}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Status",
                "instance": "Development Service"
              }
            }
          }
        ]
      },
      {
        "title": "Category 6: Marketing & Customer Experience - Engagement",
        "type": "graph",
        "gridPos": {
          "x": 16,
          "y": 24,
          "w": 8,
          "h": 6
        },
        "targets": [
          {
            "expr": "sum(rate(ai_customer_engagement_total{category=\"marketing-cx\",service=~\"personalized-marketing|ad-creation|customer-service|multilingual-support\"}[5m]))",
            "legendFormat": "Engagement Rate"
          },
          {
            "expr": "sum(rate(ai_content_generated_total{category=\"marketing-cx\"}[5m]))",
            "legendFormat": "Content/min"
          }
        ]
      },
      {
        "title": "Advanced AI Features - Model Performance",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 30,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "avg(ai_model_inference_time{category=\"advanced-features\",service=~\"voice-search|image-recognition|predictive-maintenance|smart-home|blockchain-verification|fraud-detection\"}) by (service)",
            "legendFormat": "{{service}} Inference Time (ms)"
          },
          {
            "expr": "avg(ai_model_accuracy{category=\"advanced-features\",service=~\"voice-search|image-recognition|predictive-maintenance|smart-home|blockchain-verification|fraud-detection\"}) by (service) * 100",
            "legendFormat": "{{service}} Accuracy %"
          }
        ],
        "yAxes": [
          {
            "label": "Time (ms) / Accuracy (%)"
          }
        ]
      },
      {
        "title": "Agent Productivity Tools - Automation Impact",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 30,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(ai_tasks_automated_total{category=\"agent-productivity\",service=~\"crm-automation|schedule-optimization|commission-tracking|proposal-generation\"})",
            "legendFormat": "Tasks Automated/hr"
          },
          {
            "expr": "sum(increase(ai_time_saved_minutes{category=\"agent-productivity\"}[1h]))",
            "legendFormat": "Hours Saved/hr"
          },
          {
            "expr": "avg(agent_productivity_improvement{category=\"agent-productivity\"}) * 100",
            "legendFormat": "Productivity Improvement %"
          }
        ]
      },
      {
        "title": "Global Expansion Support - International Features",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 38,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(ai_currency_conversions_total{category=\"global-expansion\"}[5m]))",
            "legendFormat": "Conversions/min"
          },
          {
            "expr": "sum(rate(ai_translations_processed_total{category=\"global-expansion\"}[5m]))",
            "legendFormat": "Translations/min"
          },
          {
            "expr": "sum(ai_compliance_checks_passed_total{category=\"global-expansion\"}[5m]))",
            "legendFormat": "Compliance Checks/min"
          }
        ],
        "yAxes": [
          {
            "label": "Operations per Minute"
          }
        ]
      },
      {
        "title": "Business Impact - AI-Driven Revenue",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 38,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(gogidix_financial_total_revenue_total{source=\"ai-generated\"}[1h]))",
            "legendFormat": "AI-Generated Revenue (hourly)"
          },
          {
            "expr": "sum(increase(gogidix_business_bookings_total{ai_assisted=\"true\"}[1h]))",
            "legendFormat": "AI-Assisted Bookings/hr"
          },
          {
            "expr": "avg(gogidix_business_conversion_rate{ai_enhanced=\"true\"}) * 100",
            "legendFormat": "AI-Enhanced Conversion Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Revenue / Bookings / Rate (%)"
          }
        ]
      },
      {
        "title": "AI Model Performance - Top Performing Models",
        "type": "table",
        "gridPos": {
          "x": 0,
          "y": 46,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "topk(10, ai_model_accuracy)",
            "legendFormat": "{{model_name}}"
          },
          {
            "expr": "topk(10, ai_model_inference_time)",
            "legendFormat": "{{model_name}}"
          },
          {
            "expr": "topk(10, ai_model_requests_total)",
            "legendFormat": "{{model_name}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Performance",
                "model_name": "AI Model"
              }
            }
          }
        ],
        "styles": [
          {
            "color": {
              "mode": "thresholds"
            },
            "date": false,
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "index": 0,
                    "text": "Poor"
                  },
                  "0.7": {
                    "color": "yellow",
                    "index": 1,
                    "text": "Good"
                  },
                  "0.9": {
                    "color": "green",
                    "index": 2,
                    "text": "Excellent"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        ]
      },
      {
        "title": "AI Resource Utilization",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 54,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(ai_gpu_utilization_percent{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "GPU Utilization %"
          },
          {
            "expr": "sum(rate(ai_memory_utilization_percent{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "Memory Utilization %"
          },
          {
            "expr": "sum(rate(ai_model_cache_hit_rate{job=\"gogidix-ai-services\"}[5m])) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Utilization Percentage",
            "max": 100,
            "min": 0
          }
        ]
      },
      {
        "title": "AI Service Health & Errors",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 54,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(ai_errors_total{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "AI Errors/sec"
          },
          {
            "expr": "sum(rate(ai_timeouts_total{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "AI Timeouts/sec"
          },
          {
            "expr": "sum(rate(ai_fallbacks_triggered_total{job=\"gogidix-ai-services\"}[5m]))",
            "legendFormat": "Fallbacks/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Events per Second"
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 17,
    "style": "dark",
    "templating": {
      "list": [
        {
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "label": "AI Service",
          "multi": true,
          "name": "ai_service",
          "options": [],
          "query": "label_values(up{job=\"gogidix-ai-services\"}, service)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "type": "query"
        },
        {
          "current": {
            "text": "All Categories",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "label": "AI Category",
          "multi": true,
          "name": "ai_category",
          "options": [],
          "query": "label_values(up{job=\"gogidix-ai-services\"}, category)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "type": "query"
        },
        {
          "current": {
            "text": "All Models",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "label": "AI Model",
          "multi": true,
          "name": "ai_model",
          "options": [],
          "query": "label_values(ai_model_info{}, model_name)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "type": "query"
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "timezone": "",
    "title": "Gogidix AI Services - Comprehensive Intelligence Dashboard",
    "uid": "gogidix-ai-services-dashboard",
    "version": 1
  }
}